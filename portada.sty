\def\degreeyear#1{\gdef\@degreeyear{#1}}
\def\degreemonth#1{\gdef\@degreemonth{#1}}

% Title in english
\def\titleen#1{\gdef\@titleen{#1}}


% The full (unabbreviated) name of the degree
\def\degree#1{\gdef\@degree{#1}}

% The name of your advisor (AHB 00/9/18)
\def\advisor#1#2{\gdef\@advisoryy{#1} \gdef\@advisorxx{#2}}

% the institute
\def\institute#1{\gdef\@institute{#1}}

% the mastering
\def\mastering#1{\gdef\@mastering{#1}}

% the department
\def\department#1{\gdef\@department{#1}}

% the university
\def\university#1{\gdef\@university{#1}}

% the place
\def\place#1{\gdef\@place{#1}}

\newlength{\barswidth}
\setlength{\barswidth}{1.5cm}
\newlength{\totaltextwidth}
\setlength{\totaltextwidth}{\textwidth}

\addtolength{\totaltextwidth}{\barswidth}

\makeatletter
\def\maketitle{
    \pagenumbering{roman}
    \thispagestyle{empty}
    \reset@font
    \null

    \vspace{-0.6in}

    \hbox{
    	\hspace{-\barswidth}
        \vbox{
            \hsize = 0.2\totaltextwidth
            \centering
            \includegraphics[width=0.20\totaltextwidth]{logos//unsis.JPG}
        }
        \vbox{
            \hsize = 0.8\totaltextwidth
            \begin{center}
             	{\Large\bfseries\MakeUppercase\@university}\\
             	\rule{12cm}{2pt}%  height 1mm width \totaltextwidth
            \end{center}
        }

    }

    \hbox{
    	\hspace{-\barswidth}
        \vtop{
        %\vbox{
        	\hsize = \barswidth
          	\vrule width 0.2mm height 0.78\textheight depth -8mm \hspace{0.5mm}
        	\vrule width 1mm height 0.82\textheight depth 0mm \hspace{0.5mm}
        	\vrule width 0.2mm height 0.78\textheight depth -8mm \hspace{0.5mm}%
        }
	\vbox{
		%\hsize = \barswidth
        \hsize = 0.9\textwidth
            \begin{center}
            	\vskip 15\p@
            	\MakeUppercase\@title\\
            	\vskip 50\p@
            	\large T\hspace{2mm}E\hspace{2mm}S\hspace{2mm}I\hspace{2mm}S\\
            	\large Para obtener el título de:\\
            	\MakeUppercase\@degree\\
            	\vskip 50\p@
            	{\large Presenta:}\\
            	{\large \bfseries \MakeUppercase\@author} \par
            	\vskip 50\p@
            	{\small Bajo la dirección de:}
            	\begin{center}
            		\large \MakeUppercase\@advisoryy\\
                    \large \MakeUppercase\@advisorxx\\
            	\end{center}
            	\vskip 30\p@
            	\begin{flushright}
            		{\tiny \MakeUppercase \@place{ }\MakeUppercase \@date}
            	\end{flushright}
            	\vfil
        	\end{center}
        }

    }
    \newpage
    \hbox{}
    \thispagestyle{empty}
    \newpage
    \thispagestyle{empty}
    %\addcontentsline{toc}{section}{Página de título}
    \let\footnotesize\scriptsize
    %\let\footnoterule\relax
    %\parindent \z@
    %\reset@font
    \null
    \vskip 10\p@
    \hbox{
        \mbox{%
        \hspace{4pt}%
        \fbox{\includegraphics[width=4em]{logos//unsis.JPG}
        }%
        \hspace{4pt}
        }%
      \vrule width 2.0 mm depth 0.8\textheight \hspace{0.5mm} \vrule width 1.0mm depth 0.8\textheight%
      \mbox{\hspace{2em}}
      \vtop{% %%%%%%%%%%%%%%%%%%
        \hsize = 4in
        \vskip 40\p@
        \begin{flushleft}
          \Large \@author \par
        \end{flushleft}
        \vskip 80\p@
        \begin{flushleft}
          \LARGE \bfseries \@title \par
        \end{flushleft}
        \vfil
        }}
}


\makeatother
